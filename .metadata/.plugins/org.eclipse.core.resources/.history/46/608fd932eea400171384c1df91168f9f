package cnam.fr.Recursive.administration;

public class Armée {

	private Armée laSeuleArmee = new Armée();

	private Armée() {
		if (laSeuleArmee != null) {
			laSeuleArmee = new Armée();
		}
	}

	public Armée getArmée() {
		return this.laSeuleArmee;
	}
	
	public String toString(){
		return "Ici c'est l'armée la seul la vraie !";
		
		
	}
	
	
}
