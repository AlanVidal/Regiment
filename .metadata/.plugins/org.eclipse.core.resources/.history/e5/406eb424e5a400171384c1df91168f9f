package cnam.fr.EX2.CaseP;

public class CasePlusDemi extends Case {

	Case source;
	int dest;

	int selfValue;

	public CasePlusDemi(char x, int y, Case aSource, int aDest) {
		super(x, y);
		this.source = aSource;
		this.dest = aDest;

	}
	@Override
	public double getValue() {
		return source.getValue() + dest;
	}

}
